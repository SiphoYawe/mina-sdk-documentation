{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mina SDK",
  "colors": {
    "primary": "#7DD3FC",
    "light": "#93C5FD",
    "dark": "#0EA5E9"
  },
  "appearance": {
