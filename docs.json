          {
            "group": "Advanced Examples",
            "pages": [
              "examples/full-integration",
              "examples/error-handling",
              "examples/event-tracking"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/siphoyawe/mina-sdk",
          "icon": "github"
        },
        {
          "anchor": "npm",
          "href": "https://www.npmjs.com/package/@siphoyawe/mina-sdk",
          "icon": "npm"
        }
      ]
    }
  },
  "logo": "/images/mina-sdk-logo.png",
  "navbar": {
    "links": [
      {
        "label": "Hyperliquid",
