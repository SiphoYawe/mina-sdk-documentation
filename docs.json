  "favicon": "/images/web-app-manifest-512x512.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "installation",
              "quickstart",
              "configuration"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "concepts/chains",
              "concepts/tokens",
              "concepts/quotes",
              "concepts/execution",
              "concepts/auto-deposit",
              "concepts/slippage"
            ]
          },
          {
            "group": "Advanced",
            "pages": [
              "advanced/caching",
              "advanced/custom-rpcs",
              "advanced/standalone"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Client",
            "pages": [
              "api/mina"
            ]
          },
          {
            "group": "Services",
            "pages": [
              "api/services/chain",
              "api/services/token",
              "api/services/balance",
              "api/services/quote",
              "api/services/execute",
              "api/services/deposit"
            ]
          },
          {
            "group": "Types",
            "pages": [
              "api/types/config",
              "api/types/chain-token",
              "api/types/quote-execution",
              "api/types/status",
              "api/types/balance"
            ]
          },
          {
            "group": "Events & Errors",
            "pages": [
              "api/events",
              "api/errors",
              "api/constants"
            ]
          }
        ]
      },
      {
        "tab": "React",
        "groups": [
          {
            "group": "Setup",
            "pages": [
              "react/installation",
              "react/provider"
            ]
          },
          {
            "group": "Hooks",
            "pages": [
              "react/hooks/use-mina",
              "react/hooks/use-quote",
              "react/hooks/use-token-balance",
              "react/hooks/use-transaction-status"
            ]
          }
        ]
      },
      {
        "tab": "Examples",
        "groups": [
          {
            "group": "Basic Examples",
            "pages": [
              "examples/simple-bridge",
              "examples/token-selection",
              "examples/quote-display"
